
<?xml version='1.0' encoding='utf-8'?>
<widget id="com.finalProject" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>ProjectConestoga</name>
  <description>Final Project for PROG2430</description>
  <author email="Mkhandakar5892@conestogac.on.ca">
    Mohammad Sadeq Khandakar
  </author>

  <author email="Yislam3947@conestogac.on.ca">
    Yeasin Islam
  </author>

  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />

  <platform name="android">
    <allow-intent href="market:*" />
  </platform>

  <icon src="logo.png" />

  <plugin name="cordova-plugin-camera" spec="^4.0.3" />
  <engine name="android" spec="^7.1.4" />

</widget>
